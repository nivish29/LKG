<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dev_until_50">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dev_until_50"
        tools:targetApi="31">
        <activity
            android:name=".project_after"
            android:exported="false" />
        <activity
            android:name=".Sign_in"
            android:exported="false" />
        <activity
            android:name=".Account_info"
            android:exported="false" />
        <activity
            android:name=".pending"
            android:exported="false"
            android:label="@string/title_activity_pending"
            android:theme="@style/Theme.Dev_until_50.NoActionBar" />
        <activity
            android:name=".all_projects"
            android:exported="false"
            android:label="@string/title_activity_all_projects"
            android:theme="@style/Theme.Dev_until_50.NoActionBar" />
        <activity
            android:name=".project_details"
            android:exported="false" />
        <activity
            android:name=".Dashboard"
            android:exported="true" />
        <activity
            android:name=".Firebase_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".user_details"
            android:exported="false" />
        <activity
            android:name=".signUpActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>